{
  "coc.preferences.colorSupport": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnType": false,
  "coc.preferences.jumpCommand": "edit",
  "coc.preferences.watchmanPath": "/usr/local/bin/watchman",
  "coc.source.around.priority": 10,
  "coc.source.buffer.priority": 11,
  "coc.source.emoji.priority": 22,
  "coc.source.file.ignoreHidden": false,
  "coc.source.file.priority": 12,
  "coc.source.word.filetypes": ["text", "markdown", "html"],
  "coc.source.word.priority": 21,
  "codeLens.enable": true,
  "diagnostic.displayByAle": true,
  "diagnostic.enable": true,
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.virtualText": true,
  "html.enable": true,
  "html.format.enable": true,

  "snippets.priority": 2,
  "snippets.trace": "verbose",
  "snippets.enableStatusItem": false,
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"],
    "ruby": ["rails"],
    "svelte": ["javascript", "typescript", "html", "css"],
    "vue": ["html", "javascript", "typescript", "css"]
  },
  "snippets.shortcut": "ᛋ",
  "snippets.autoTrigger": true,
  "snippets.triggerCharacters": [],
  "snippets.loadFromExtensions": true,
  "snippets.textmateSnippetsRoots": ["~/.config/nvim/snippets"],
  "snippets.ultisnips.enable": true,
  "snippets.ultisnips.usePythonx": true,
  "snippets.snipmate.enable": true,

  "suggest.snippetIndicator": " ",
  "suggest.floatEnable": true,
  "languageserver": {
    "elixir": {
      "command": "~/lsp/elixir-ls/language_server.sh",
      "filetypes": ["elixir", "eelixir"],
      "trace.server": "verbose",
      "rootPatterns": ["mix.lock"],
      "initializationOptions": {},
      "settings": {
        "elixirLS": {
          "dialyzerEnabled": false
        }
      }
    }
  },
  "solargraph.bundlerPath": "~/.asdf/shims/bundle",
  "solargraph.commandPath": "~/.asdf/shims/solargraph",
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue60b",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "suggest.detailMaxLength": 30,
  "suggest.enablePreview": true,
  "suggest.minTriggerInputLength": 2,
  "yank.highlight.duration": 800,
  "yank.shortcut": "\u2602"
}
