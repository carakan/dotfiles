[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  unstage = reset HEAD --
  last = log -1 HEAD
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[user]

[core]
	editor = /usr/local/bin/nvim
	excludesfile = ~/.gitignore_global
[commit]
